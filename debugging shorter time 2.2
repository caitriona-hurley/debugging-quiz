import time

start = time.time()

list = []

for a in range(1000):
    for b in range(a, 1000):
        c = 1000 - a - b
        if a+b+c == 1000:
            if a**2 + b**2 == c**2:
                list.append(a*b*c)
            else:
                continue
        else:
            continue


print(list)

end = time.time()
elapsed = end - start
print (elapsed)
